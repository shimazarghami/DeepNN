        "import numpy as np\n",
        "def cwithMSE(x,W,b):\n",
        "  y = x @ W + b\n",
        "  print(y)\n",
        "  target = 10\n",
        "  print(target)\n",
        "  loss = np.mean((y-target)**2)\n",
        "  print(loss)\n",
        "\n",
        "x = np.array([[1,2]])\n",
        "W = np.array([[2],[3]])\n",
        "b = np.array([1])\n",
        "cwithMSE(x,W,b)"
